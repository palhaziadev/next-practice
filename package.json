{
  "name": "next-practice",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "format": "prettier --check --ignore-path .gitignore .",
    "format:fix": "prettier --write --ignore-path .gitignore .",
    "husky:install": "husky install",
    "husky:add": "husky add .husky/pre-commit \"npx tsc --noEmit && npx run lint . && npx run format:fix .\"",
    "husky:add:lint": "husky add .husky/pre-commit \"npx lint-staged\"",
    "format:stylelint": "prettier --check . && stylelint --allow-empty-input \"**/*.{css,scss}\"",
    "lint:css": "stylelint **/*.{scss,css}",
    "lint:css:fix": "stylelint --fix **/*.{scss,css}",
    "init": "npm run husky:install && npm run husky:add:lint",
    "test": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:ci": "jest --ci",
    "cypress": "cypress open",
    "cypress:headless": "cypress run --headless",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "dependencies": {
    "@heroicons/react": "^2.0.18",
    "@types/node": "20.3.1",
    "@types/react": "18.2.13",
    "@types/react-dom": "18.2.6",
    "classnames": "^2.3.2",
    "dotenv": "^16.3.1",
    "eslint": "8.43.0",
    "eslint-config-next": "13.4.6",
    "firebase": "^10.1.0",
    "mobx": "^6.9.0",
    "mobx-react-lite": "^3.4.3",
    "next": "13.4.6",
    "next-intl": "^3.0.0-beta.9",
    "next-themes": "^0.2.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "typescript": "5.1.3"
  },
  "devDependencies": {
    "@storybook/addon-essentials": "^7.4.0",
    "@storybook/addon-interactions": "^7.4.0",
    "@storybook/addon-links": "^7.4.0",
    "@storybook/addon-onboarding": "^1.0.8",
    "@storybook/blocks": "^7.4.0",
    "@storybook/nextjs": "^7.4.0",
    "@storybook/react": "^7.4.0",
    "@storybook/testing-library": "^0.2.0",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.4.3",
    "@typescript-eslint/eslint-plugin": "^5.60.0",
    "autoprefixer": "^10.4.14",
    "cypress": "^13.2.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-cypress": "^2.15.1",
    "eslint-plugin-storybook": "^0.6.13",
    "husky": "^8.0.3",
    "jest": "^29.5.0",
    "jest-environment-jsdom": "^29.5.0",
    "lint-staged": "^13.2.2",
    "next-router-mock": "^0.9.9",
    "postcss": "^8.4.24",
    "prettier": "^2.8.8",
    "sass": "^1.63.5",
    "storybook": "^7.4.0",
    "stylelint": "^15.8.0",
    "stylelint-config-prettier-scss": "^1.0.0",
    "stylelint-config-standard-scss": "^9.0.0",
    "stylelint-order": "^6.0.3",
    "stylelint-scss": "^5.0.1",
    "tailwindcss": "^3.3.2"
  }
}
